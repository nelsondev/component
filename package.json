	},
	"homepage": "https://github.com/nelsondev/component#readme",
	"devDependencies": {
		"@rollup/plugin-commonjs": "^26.0.1",
		"@rollup/plugin-node-resolve": "^15.2.3",
		"@rollup/plugin-terser": "^0.4.4",
		"rollup": "^4.21.2"
	},
	"engines": {
		"node": ">=16.0.0"
	},
	"browserslist": [
		"> 1%",
